<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd">
	<param name="id" value="#{categoriaHome.categoriaId}" required="false" />
	<action execute="#{categoriaHome.load()}" on-postback="false" />
	<navigation from-action="#{categoriaHome.save()}">
		<rule if-outcome="true">
			<redirect view-id="/sainv/categoria/list.xhtml" />
		</rule>
	</navigation>
	
	<navigation from-action="#{categoriaHome.modify()}">
		<rule if-outcome="true">
			<redirect view-id="/sainv/categoria/list.xhtml" />
		</rule>
	</navigation>
</page> 