<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd">
	<param name="id" value="#{transferenciaHome.transferenciaId}" required="false" />
	<action execute="#{transferenciaHome.load()}" on-postback="false" />
	
	<navigation from-action="#{transferenciaHome.save()}">
		<rule if-outcome="true">
			<redirect view-id="/sainv/transferencia/list.xhtml" />
		</rule>
	</navigation>
	
	<navigation from-action="#{transferenciaHome.modify()}">
		<rule if-outcome="true">
			<redirect view-id="/sainv/transferencia/list.xhtml" />
		</rule>
		<rule if-outcome="false">
			<redirect view-id="/sainv/transferencia/list.xhtml" />
		</rule>
	</navigation>
	
	<navigation from-action="#{transferenciaHome.productosRecibidos()}">
		<rule if-outcome="true">
			<redirect view-id="/sainv/transferencia/list.xhtml" />
		</rule>
		<rule if-outcome="false">
			<redirect view-id="/sainv/transferencia/master.xhtml" />
		</rule>
	</navigation>
</page>